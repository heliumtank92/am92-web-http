{
  "name": "@am92/web-http",
  "version": "2.0.2",
  "description": "HTTP Client for Web Frontend Applications",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "compile": "tsc -b ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./dist",
    "build": "npm-run-all build:clean compile",
    "test": "echo 'Error: no test specified'",
    "preversion": "npm-run-all build test",
    "version": "echo 'Versioning'",
    "postversion": "git push && git push --tags && npm run publish:pkg",
    "publish:pkg": "npm publish"
  },
  "dependencies": {
    "@am92/jose-crypto-subtle": "^2.0.1",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0"
  },
  "repository": "git@github.com:heliumtank92/am92-web-http.git",
  "homepage": "https://github.com/heliumtank92/am92-web-http#readme",
  "contributors": [
    {
      "name": "Ankit Gandhi",
      "email": "ankitgandhi452@gmail.com",
      "url": "https://github.com/ankitgandhi452"
    },
    {
      "name": "Mehul Agarwal",
      "email": "agarwalmehul92@gmail.com",
      "url": "https://github.com/agarwalmehul"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "web http",
    "axios",
    "http",
    "http client",
    "request",
    "response"
  ]
}
